{
  "extends": "airbnb-base",
  "env": {
    "node": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/extensions": ["error", "ignorePackages"],
    "linebreak-style": "off",
    "import/prefer-default-export": "off",
    "no-console": "warn",
    "camelcase": "warn",
    "arrow-body-style": "warn",
    "arrow-parens": "warn",
    "comma-dangle": ["error", "always-multiline"]
  }
}
